      </HBox>
      <HBox spacing="20.0">
         <children>
            <Label text="Port" />
            <TextField fx:id="portTextField" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
